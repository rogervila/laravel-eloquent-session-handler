{
    "name": "rogervila/laravel-eloquent-session-handler",
    "description": "Eloquent session handler for Laravel",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel eloquent session",
        "laravel session model"
    ],
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "rogervila@me.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/framework": "^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "symfony/filesystem": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelEloquentSessionHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\LaravelEloquentSessionHandler\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEloquentSessionHandler\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
